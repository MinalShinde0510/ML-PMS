@model System.Data.DataTable
@using System.Data

@{
    ViewBag.Title = "Dashboard";
}
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>53<sup style="font-size: 20px">%</sup></h3>
                        <p>Progress </p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-stats-bars"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3>@TempData["TotalPRoject"]</h3>
                        <p>New Projects</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-bag"></i>
                    </div>
                    <a data-target="#ProjectModal" data-toggle="modal" href="#ProjectModal" class="small-box-footer" id="Project">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="ProjectModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Project</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Project Name </th>
                                <th>Location </th>
                                <th>Start Date</th>
                                <th>End Date</th>
                            </tr>
                            @if (ViewBag.ProjectDetailsList != null)
                            {
                                foreach (var item in ViewBag.ProjectDetailsList)
                                {
                                    <tr>
                                        <td>@item.ProjectName</td>
                                        <td>@item.Location</td>
                                        <td>@item.StartDate</td>
                                        <td>
                                            @if (item.EndDate != null)
                                            {
                                                @item.EndDate
                                            }
                                        </td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-blue-active">
                    <div class="inner">
                        <h3>@TempData["TotalCompany"]</h3>
                        <p>Company</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-woman"></i>
                    </div>
                    <a data-target="#CompanyModal" data-toggle="modal" href="#CompanyModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="CompanyModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Company</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Company Name</th>
                                <th>City </th>
                                <th>Contact No</th>
                                <th>E Mail</th>
                                <th>GST IN</th>
                            </tr>
                            @if (ViewBag.CompanyDetailsList != null)
                            {
                                foreach (var item in ViewBag.CompanyDetailsList)
                                {
                                    <tr>
                                        <td>@item.CompanyName</td>
                                        <td>@item.City</td>
                                        <td>@item.ContactNo</td>
                                        <td>@item.EMail</td>
                                        <td>@item.GSTIN</td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-yellow">
                    <div class="inner">
                        <h3>@TempData["TotalClient"]</h3>
                        <p>Client</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-person-add"></i>
                    </div>
                    <a data-target="#ClientModal" data-toggle="modal" href="#ClientModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="ClientModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Client</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Client Name</th>
                                <th>City </th>
                                <th>Contact No</th>
                                <th>E Mail</th>
                                <th>GST IN</th>
                            </tr>
                            @if (ViewBag.ClientDetailsList != null)
                            {
                                foreach (var item in ViewBag.ClientDetailsList)
                                {
                                    <tr>
                                        <td>@item.ClientName</td>
                                        <td>@item.City</td>
                                        <td>@item.ContactNo</td>
                                        <td>@item.EMail</td>
                                        <td>@item.GSTIN</td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-red">
                    <div class="inner">
                        <h3>@TempData["TotalEmployee"]</h3>
                        <p>Employees</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-pie-graph"></i>
                    </div>
                    <a data-target="#EmployeeModal" data-toggle="modal" href="#EmployeeModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="EmployeeModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Employee</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>D O B</th>
                                <th>D O J</th>
                                <th>Gender</th>
                                <th>E Mail</th>
                                <th>Mobile</th>
                                <th>Salary</th>
                            </tr>
                            @if (ViewBag.EmployeeDetailsList != null)
                            {
                                foreach (var item in ViewBag.EmployeeDetailsList)
                                {
                                    <tr>
                                        <td>@item.FirstName</td>
                                        <td>@item.LastName</td>
                                        <td>@item.DOB</td>
                                        <td>@item.DOJ</td>
                                        <td>@item.Gender</td>
                                        <td>@item.EMail</td>
                                        <td>@item.Mobile</td>
                                        <td>@item.Salary</td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-olive-active">
                    <div class="inner">
                        <h3>@TempData["TotalInvoice"]</h3>
                        <p>Invoice</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-information"></i>
                    </div>
                    <a data-target="#InvoiceModal" data-toggle="modal" href="#InvoiceModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="InvoiceModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Invoice</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Invoice No.</th>
                                <th>Invoice Date </th>
                                <th>Client Name</th>
                                <th>GrandTotal</th>
                            </tr>
                            @if (ViewBag.InvoiceDetailsList != null)
                            {
                                foreach (var item in ViewBag.InvoiceDetailsList)
                                {
                                    <tr>
                                        <td>@item.InvoiceNo</td>
                                        <td>@item.InvoiceDate</td>
                                        <td>@item.ClientName</td>
                                        <td>@item.GrandTotal</td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-purple-active">
                    <div class="inner">
                        <h3>@TempData["TotalTaskManagement"]</h3>
                        <p>Task Management</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-android-drafts"></i>
                    </div>
                    <a data-target="#TaskManagementModal" data-toggle="modal" href="#TaskManagementModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="TaskManagementModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Task Management</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Assignment Date</th>
                                <th>Task Name</th>
                                <th>First Name</th>
                                <th>Task State Name</th>
                            </tr>
                            @if (ViewBag.TaskManagementDetailsList != null)
                            {
                                foreach (var item in ViewBag.TaskManagementDetailsList)
                                {
                                    <tr>
                                        @*<td>@item.AssignmentDate</td>
                                            <td>@item.TaskName</td>
                                            <td>@item.FirstName</td>
                                            <td>@item.TaskStateName</td>*@
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-teal-active">
                    <div class="inner">
                        <h3>@TempData["TotalTimeSheet"]</h3>
                        <p>Time Sheet</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-clock"></i>
                    </div>
                    <a data-target="#TimeSheetModal" data-toggle="modal" href="#TimeSheetModal" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @* Modal PopUp for adding New Item Details*@
            <div class="modal fade" tabindex="-1" id="TimeSheetModal" data-keyboard="false" data-backdrop="static">
                @*<div class="modal-dialog modal-lg">*@
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            ×
                        </button>
                        <h4 class="modal-title">Time Sheet</h4>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <tr class="bg-warning">
                                <th>Employee</th>
                                <th>Project </th>
                                <th>Entry Date</th>
                                <th>StartTime</th>
                                <th>EndTime</th>
                                <th>Remarks</th>
                            </tr>
                            @if (ViewBag.TimeSheetDetailsList != null)
                            {
                                foreach (var item in ViewBag.TimeSheetDetailsList)
                                {
                                    <tr>
                                        <td>@item.Employee</td>
                                        <td>@item.Project</td>
                                        <td>@item.EntryDate</td>
                                        <td>@item.StartTime</td>
                                        <td>@item.EndTime</td>
                                        <td>@item.Remarks</td>
                                    </tr>
                                }
                            }
                        </table>
                    </div>
                </div>
                @*</div>*@
            </div>
            @* END Modal PopUp for adding New Item Details*@
            <!-- ./col -->
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-7 connectedSortable">
                <!-- Custom tabs (Charts with tabs)-->
                <div class="nav-tabs-custom">
                    <!-- Tabs within a box -->
                    <ul class="nav nav-tabs pull-right">
                        <li class="active"><a href="#revenue-chart" data-toggle="tab">Area</a></li>
                        <li><a href="#sales-chart" data-toggle="tab">Donut</a></li>
                        <li class="pull-left header"><i class="fa fa-inbox"></i> Sales</li>
                    </ul>
                    <div class="tab-content no-padding">
                        <!-- Morris chart - Sales -->
                        <div class="chart tab-pane active" id="revenue-chart" style="position: relative; height: 300px;"></div>
                        <div class="chart tab-pane" id="sales-chart" style="position: relative; height: 300px;"></div>
                    </div>
                </div>
                <!-- /.nav-tabs-custom -->
                <!-- Chat box -->
                <div class="box box-success">
                    <div class="box-header">
                        <i class="fa fa-comments-o"></i>
                        <h3 class="box-title">Chat</h3>
                        <div class="box-tools pull-right" data-toggle="tooltip" title="Status">
                            <div class="btn-group" data-toggle="btn-toggle">
                                <button type="button" class="btn btn-default btn-sm active">
                                    <i class="fa fa-square text-green"></i>
                                </button>
                                <button type="button" class="btn btn-default btn-sm"><i class="fa fa-square text-red"></i></button>
                            </div>
                        </div>
                    </div>
                    <div class="box-body chat" id="chat-box">
                        <!-- chat item -->
                        <div class="item">
                            <img src="Content/adminlte/img/user4-128x128.jpg" alt="user image" class="online">
                            <p class="message">
                                <a href="#" class="name">
                                    <small class="text-muted pull-right"><i class="fa fa-clock-o"></i> 2:15</small>
                                    Mike Doe
                                </a>
                                I would like to meet you to discuss the latest news about
                                the arrival of the new theme. They say it is going to be one the
                                best themes on the market
                            </p>
                            <div class="attachment">
                                <h4>Attachments:</h4>
                                <p class="filename">
                                    Theme-thumbnail-image.jpg
                                </p>
                                <div class="pull-right">
                                    <button type="button" class="btn btn-primary btn-sm btn-flat">Open</button>
                                </div>
                            </div>
                            <!-- /.attachment -->
                        </div>
                        <!-- /.item -->
                        <!-- chat item -->
                        <div class="item">
                            <img src="Content/adminlte/img/user3-128x128.jpg" alt="user image" class="offline">
                            <p class="message">
                                <a href="#" class="name">
                                    <small class="text-muted pull-right"><i class="fa fa-clock-o"></i> 5:15</small>
                                    Alexander Pierce
                                </a>
                                I would like to meet you to discuss the latest news about
                                the arrival of the new theme. They say it is going to be one the
                                best themes on the market
                            </p>
                        </div>
                        <!-- /.item -->
                        <!-- chat item -->
                        <div class="item">
                            <img src="Content/adminlte/img/user2-160x160.jpg" alt="user image" class="offline">
                            <p class="message">
                                <a href="#" class="name">
                                    <small class="text-muted pull-right"><i class="fa fa-clock-o"></i> 5:30</small>
                                    Susan Doe
                                </a>
                                I would like to meet you to discuss the latest news about
                                the arrival of the new theme. They say it is going to be one the
                                best themes on the market
                            </p>
                        </div>
                        <!-- /.item -->
                    </div>
                    <!-- /.chat -->
                    <div class="box-footer">
                        <div class="input-group">
                            <input class="form-control" placeholder="Type message...">
                            <div class="input-group-btn">
                                <button type="button" class="btn btn-success"><i class="fa fa-plus"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box (chat box) -->
                <!-- TO DO List -->
                <div class="box box-primary">
                    <div class="box-header">
                        <i class="ion ion-clipboard"></i>
                        <h3 class="box-title">To Do List</h3>
                        <div class="box-tools pull-right">
                            <ul class="pagination pagination-sm inline">
                                <li><a href="#">&laquo;</a></li>
                                <li><a href="#">1</a></li>
                                <li><a href="#">2</a></li>
                                <li><a href="#">3</a></li>
                                <li><a href="#">&raquo;</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <!-- See dist/js/pages/dashboard.js to activate the todoList plugin -->
                        <ul class="todo-list">
                            <li>
                                <!-- drag handle -->
                                <span class="handle">
                                    <i class="fa fa-ellipsis-v"></i>
                                    <i class="fa fa-ellipsis-v"></i>
                                </span>
                                <!-- checkbox -->
                                <input type="checkbox" value="">
                                <!-- todo text -->
